{
  "manifest_version": 2,
  "name": "Envatos",
  "description": "An extension from kenek's X-projects",
  "version": "1.0",
  "homepage_url": "http://kenek.xyz",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "src/assets/index.js"
      ]
    }
  ],
  "options_page": "src/options.html",
  "permissions": [
    "storage"
  ]
}
